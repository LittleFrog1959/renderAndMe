<body>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous">
    </script>

    <script>
        window.onload = function () {
            var canvas = document.getElementById('courtCanvas');
            var context = canvas.getContext('2d');
            var image = new Image();
            image.src = './images/bblCourt.png';
            image.onload = function() {
                context.drawImage(image, 0, 0);
            };
        
            canvas.addEventListener ('mousedown', function (e) {
                // Redraw the whole image (!) to erase the old dot
                context.drawImage(image, 0, 0);
                // Express the X & Y positions as a percentage of the canvas width
                // with the ogigin offsets removed
                xPercent = (e.clientX - e.srcElement.offsetLeft) / e.srcElement.clientWidth
                yPercent = (e.clientY - e.srcElement.offsetTop) / e.srcElement.clientHeight
                // Now scale by the current size of the canvas
                x = xPercent * e.srcElement.width;
                y = yPercent * e.srcElement.height;

                // Tell the rest of the program so the position can be sent to the back end
                document.getElementsByName('xPosition')[0].value = xPercent;
                document.getElementsByName('yPosition')[0].value = yPercent;

                // Plot that naughty dot on the screen
                context.beginPath ()
                context.arc (x, y, 2, 0, Math.PI * 2)
                context.fillStyle = "red";
                context.fill();
                context.lineWidth = 2;
                context.strokeStyle = "blue";
                context.stroke ()
            }) 
            // Set up a call every 500mSec which refreshes the various on-screen times etc...
            setInterval (refreshTimes, 500)
            
            // Expand the supplied JSON object and plant it in the textarea
            document.getElementById ("eventDump").value = '<%-msg%>'
            // Refresh the hidden values used by HTML POST
            loadHiddenFieldNames ('gameStartName', '<%= gStart %>')
            loadHiddenFieldNames ('cQtrName', '<%= cQtr %>')
            loadHiddenFieldNames ('qtrStartName', '<%= qtrStart %>')
            loadHiddenFieldNames ('gameEndName', '<%= gEnd %>')
                        
            document.getElementById ("gameStartButton").onclick = function () {
                // Take a snapshot of the current time
                var rightNow = new Date ().toLocaleString ('en-GB')
                
                // Load the on-screen text boxes
                document.getElementById ("gameStartedAt").value = rightNow
                document.getElementById ("currentQuarter").value = "1st Quarter started"
                document.getElementById ("quarterStarted").value = rightNow
                document.getElementById ("quarterDuration").value = "0"
                document.getElementById ("gameEndedAt").value = ""
                // Load up the element included in messages to the server
                loadHiddenFieldNames ('wallClockName', rightNow)
                loadHiddenFieldNames ("gameStartName", rightNow)
                loadHiddenFieldNames ('cQtrName', '1st Quarter started')
                loadHiddenFieldNames ('qtrStartName', rightNow)
                loadHiddenFieldNames ('gameEndName', '')
                // Get the current duration of the quarter from the screen
                // for sending to the server
                var qD = document.getElementById ('quarterDuration').value
                loadHiddenFieldNames ('qtrDurationName', qD)
            }

            document.getElementById ("qtr1EndButton").onclick = function () {
                // Load the on-screen message
                document.getElementById ("currentQuarter").value = "1st Quarter ended"
                // Load up the element included in messages to the server
                loadHiddenFieldNames ('cQtrName', '1st Quarter ended')
            }   

            document.getElementById ("qtr2StartButton").onclick = function () {
                 // Take a snapshot of the current time
                var rightNow = new Date ().toLocaleString ('en-GB')
                // Load the on-screen message
                document.getElementById ("currentQuarter").value = "2nd Quarter started"
                document.getElementById ("quarterStarted").value = rightNow
                document.getElementById ("quarterDuration").value = "0"
                // Load up the elements included in messages to the server
                loadHiddenFieldNames ('cQtrName', '2nd Quarter started')
                loadHiddenFieldNames ('qtrStartName', rightNow)
                // Get the current duration of the quarter from the screen
                // for sending to the server
                var qD = document.getElementById ('quarterDuration').value
                loadHiddenFieldNames ('qtrDurationName', qD)
            }   

            document.getElementById ("qtr2EndButton").onclick = function () {
                // Load the on-screen message
                document.getElementById ("currentQuarter").value = "2nd Quarter ended"
                // Load up the element included in messages to the server
                loadHiddenFieldNames ('cQtrName', '2nd Quarter ended')
            }   

            document.getElementById ("qtr3StartButton").onclick = function () {
                 // Take a snapshot of the current time
                var rightNow = new Date ().toLocaleString ('en-GB')
                // Load the on-screen message
                document.getElementById ("currentQuarter").value = "3rd Quarter started"
                document.getElementById ("quarterStarted").value = rightNow
                document.getElementById ("quarterDuration").value = "0"
                // Load up the elements included in messages to the server
                loadHiddenFieldNames ('cQtrName', '3rd Quarter started')
                loadHiddenFieldNames ('qtrStartName', rightNow)
                // Get the current duration of the quarter from the screen
                // for sending to the server
                var qD = document.getElementById ('quarterDuration').value
                loadHiddenFieldNames ('qtrDurationName', qD)
            }
            
            document.getElementById ("qtr3EndButton").onclick = function () {
                // Load the on-screen message
                document.getElementById ("currentQuarter").value = "3rd Quarter ended"
                // Load up the element included in messages to the server
                loadHiddenFieldNames ('cQtrName', '3rd Quarter ended')
            }   

            document.getElementById ("qtr4StartButton").onclick = function () {
                 // Take a snapshot of the current time
                var rightNow = new Date ().toLocaleString ('en-GB')
                // Load the on-screen message
                document.getElementById ("currentQuarter").value = "4th Quarter started"
                document.getElementById ("quarterStarted").value = rightNow
                document.getElementById ("quarterDuration").value = "0"
                // Load up the elements included in messages to the server
                loadHiddenFieldNames ('cQtrName', '4th Quarter started')
                loadHiddenFieldNames ('qtrStartName', rightNow)
                // Get the current duration of the quarter from the screen
                // for sending to the server
                var qD = document.getElementById ('quarterDuration').value
                loadHiddenFieldNames ('qtrDurationName', qD)
            }
            
            document.getElementById ("gameEndButton").onclick = function () {
                // Take a snapshot of the current time
                var rightNow = new Date ().toLocaleString ('en-GB')
                
                // Load the on-screen text boxes
                document.getElementById ("currentQuarter").value = "Game ended"
                document.getElementById ("gameEndedAt").value = rightNow
                // Load up the element included in messages to the server
                loadHiddenFieldNames ('cQtrName', 'Game ended')
                loadHiddenFieldNames ('gameEndName', rightNow)
                // Get the current duration of the quarter from the screen
                // for sending to the server
                var qD = document.getElementById ('quarterDuration').value
                loadHiddenFieldNames ('qtrDurationName', qD)
            }
        }
        
        function loadHiddenFieldNames (fieldName, fieldValue) {
            let nameList = document.getElementsByName (fieldName);
            nameList.forEach ((element) => {
                element.value = fieldValue
            });
        }
        function refreshTimes () {
            // Refresh the various on-screen times
            // Wall clock date/time
            var rightNow = new Date ().toLocaleString ('en-GB')            
            document.getElementById ('wallClockTime').value = rightNow 
            // Send the current wall clock time to all the HTML POST fields
            loadHiddenFieldNames ('wallClockName', rightNow)
            // Update the duration of the current quarter if there's a valid
            // quarter start time AND the quarter is in play (in other words,
            // not "ended")
            let start = document.getElementById ("quarterStarted").value
            let quarterState = document.getElementById ("currentQuarter").value
            if (quarterState.includes ('started') === true) {
                if (start.length === 20) {
                    let dur = getSeconds (rightNow) - getSeconds (start)
                    document.getElementById ("quarterDuration").value = dur
                    loadHiddenFieldNames ('qtrDurationName', dur)
                } else {
                    let dur = ""
                    // Clear out the duration because start time is not valid
                    document.getElementById ("quarterDuration").value = ""
                    loadHiddenFieldNames ('qtrDurationName', "")

                }
            }
        }
        function getSeconds (localeString) {
            // Long handed way of getting seconds from supplied date/time
            let h = parseInt (localeString.slice (12, 14)) * 3600
            let m = parseInt (localeString.slice (15, 17)) * 60
            let s = parseInt (localeString.slice (-2))
            return (h + m + s)
        }        
    </script>
    
